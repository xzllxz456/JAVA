
public class mainClass {
	public static void main(String[] args) {
		/*
		 * random: 난수, 무작위 <--> pattern
		 */
		int randNum;
		
		randNum = (int)(Math.random() * 10);	//0~9
		System.out.println("randNum: " + randNum);
		
		//10~19
		randNum = (int)(Math.random() * 10) + 10;
		System.out.println("randNum: " + randNum);
	}
}
